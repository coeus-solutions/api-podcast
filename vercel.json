{
    "version": 2,
    "functions": {
        "api/main.py": {
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "MAX_BODY_SIZE": "100mb"
    }
} 